    <node pkg="tf2_ros_laser" type="static_transform_publisher" args=" 0 0 0 2.97 0 0 base_link laser" name="static_transform_publisher"/>
    <node pkg="tf2_ros_odom" type="static_transform_publisher" args=" 0 0 0 0 0 0 base_link odom" name="static_transform_publisher"/>
